<h1>Show Courses</h1>

<table class="table table-bordered table-striped">
    <thead>
        <th>course id</th>
        <th>name</th>
    </thead>
    <tbody>
        <% for(course of courses){%> 
        <tr> 
            <td><%= course.subject %> <%= course.coursenum %></td> 
            <td><%= course.name %> </td> 
        </tr>
        <% } %>
    </tbody>
</table>

<pre><%= JSON.stringify(courses[0],null,5) %> </pre>
